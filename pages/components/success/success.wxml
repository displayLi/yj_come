<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">{{title}}</view>
            <view class="weui-msg__desc"><slot name="content"></slot></view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary" bindtap="btnFun">{{btnName}}</button>
                <button class="weui-btn" type="default" bindtap="goIndex">随便逛逛</button>
            </view>
        </view>
    </view>
</view>