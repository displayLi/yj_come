.container{
    background-color: #f1f1f1;
}
.goods-list{
    background-color: #f8f8f8;
    padding: 16rpx 20rpx;
    margin-bottom: 30rpx;
    border-bottom: #d9d9d9 1px solid;
}
.goods-list-img{
    width: 140rpx;
    height: 140rpx;
    margin-right: 20rpx;
}
.goods-list-img image{
    width: 100%;
    height: 100%;
}
.goods-list-ct{
    font-size: 24rpx;
    line-height: 34rpx;
    overflow: hidden;
}
.goods-list-fr{
    text-align: right;
    margin-left: 30rpx;
    font-size: 24rpx;
    line-height: 34rpx;
}