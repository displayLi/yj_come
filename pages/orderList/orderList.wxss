page{
    background-color: #ebebeb;
}
.shoppingCart-ttBox{
    height: 88rpx;
}
.shoppingCart-tt{
    height: 88rpx;
    padding: 0 30rpx;
    line-height: 88rpx;
    font-size: 30rpx;
    color: #060606;
    background-color: #ffffff;
    border-bottom: #e9e9e9 1px solid;
    border-top: #e9e9e9 1px solid;
}
.shoppingCart-tt icon{
    margin-right: 8rpx;
    font-size: 32rpx;
}
.shoppingCart-img{
    float: left;
    width: 190rpx;
    height: 190rpx;
    margin-right: 20rpx;
}
.shoppingCart-img image{
    width: 100%;
    height: 100%;
}
.shoppingCart-box-ct1{
    font-size: 28rpx;
    color: #060606;
    line-height: 38rpx;
    height: 76rpx;
}
.shoppingCart-box-ct2{
    font-size: 24rpx;
    margin: 16rpx 0;
    color: #8a8a8a;
}
.shoppingCart-box-ct3{
    font-size: 34rpx;
    color: #d6000f;
}
.shoppingCart-box-ct3 text{
    font-size: 24rpx;
}
.shoppingCart-box-ct3 text.fr{
    color: #8a8a8a;
}

.orderList{
  margin-top: 83rpx;
}

.orderList-nav{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-bottom: #d9d9d9 1px solid;
    z-index: 99;
    background-color: #ffffff;
}
.orderList-nav .act{
    color: #d6000f;
    border-bottom: #d6000f 1px solid;
}
.orderList-cell{
    width: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}
.orderList-box{
    margin-top: 30rpx;
}
.orderList-box:first-child{
    margin-top: 0;
}
.orderList-scroll{
    padding-top: 80rpx;
    height: 100%;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

.orderList-form{
    font-size: 28rpx;
    margin-bottom: 0;
    text-align: right;
}
.orderList-form:before{
    border: none 0;
}
.orderList-btn button{
    margin-top: 0;
    margin-left: 20rpx;
}
.orderList-btn navigator{
  position:relative;
  display:inline-block;
  margin-left:20rpx;
  padding: 0 1.34em;
  box-sizing:border-box;
  font-size: 32rpx;
  text-align:center;
  text-decoration:none;
  line-height:2.3;
  border-radius:5px;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
  color:#000000;
  background-color:#F8F8F8;
}